  "components": {
    "schemas": {
      "DtoDatasetList": {
        "type": "object",
        "properties": {
          "results": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "DtoEncodeTypeList": {
        "type": "object",
