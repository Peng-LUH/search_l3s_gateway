              "type": "string"
            }
          }
        }
      },
      "DtoDocumentList": {
        "type": "object",
        "properties": {
          "secret": {
            "type": "string"
          },
          "documents": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/DtoDocument"
            }
          }
        }
      },
      "DtoDocument": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "owner": {
            "type": "array",
            "description": "List of orga-id",
            "items": {
              "type": "string"
            }
          },
          "entity_id": {
            "type": "string"
          },
          "entity_id_full": {
            "type": "string"
          },
          "entity_type": {
            "type": "string",
            "example": "skill",
            "enum": [
              "skill",
              "path",
              "task"
            ]
          },
          "contents": {
            "type": "string"
          }
        }
      },
      "DtoSearcherUpdateResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        }
      },
      "DtoDenseSearchRequest": {
        "required": [
          "query"
        ],
        "type": "object",
        "properties": {
          "user_id": {
            "type": "string",
            "description": "user ID"
          },
          "owner": {
            "type": "array",
            "description": "company ID",
            "items": {
              "type": "string"
            }
          },
          "query": {
            "type": "string",
            "default": "Elektrotechnik 1 Versuch 8: Wirkleistung von Wechselspannungen; Wirkleistung der Sinuswechselspannung in der praktischen Ãœbung"
          },
          "use_skill_profile": {
            "type": "boolean",
            "default": false
          },
          "user_learning_profile": {
            "type": "boolean",
            "default": false
          },
          "language_model": {
            "type": "string",
            "default": "bert-base-german-cased"
          },
          "index_method": {
            "type": "string",
            "example": "flat-l2",
            "default": "flat-l2",
            "enum": [
              "flat-l2",
              "flat-ip"
            ]
          },
          "entity_type": {
            "type": "string",
            "example": "all",
            "enum": [
              "all",
              "skill",
              "path",
              "task"
            ]
          },
          "nr_result": {
            "minimum": 0,
            "type": "integer"
          }
        }
      },
      "DtoDenseSearchResponseList": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          },
          "results": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/DtoDenseSearchResponse"
            }
          }
        }
      },
      "DtoDenseSearchResponse": {
        "type": "object",
        "properties": {
          "user_id": {
            "type": "string",
            "description": "user ID"
          },
          "owner": {
            "type": "string",
            "description": "company ID"
          },
          "entity_id": {
            "type": "string"
          },
          "entity_type": {
            "type": "string"
          },
          "similarity": {
            "type": "number"
          }
        }
      }
    },
    "responses": {
      "ParseError": {
        "description": "When a mask can't be parsed",
        "content": {}
      },
      "MaskError": {
        "description": "When any error occurs on mask",
