  "components": {
    "schemas": {
      "DtoDatasetList": {
        "type": "object",
        "properties": {
          "results": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/DtoDataset"
            }
          }
        }
      },
      "DtoDataset": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "name of dataset"
          }
        }
      },
      "DtoEncodeTypeList": {
        "type": "object",
        "properties": {
          "results": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/DtoEncodeType"
            }
          }
        }
      },
      "DtoEncodeType": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "name of encode type"
          }
        }
      },
      "DtoLanguageModelList": {
        "type": "object",
        "properties": {
          "results": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/DtoLanguageModel"
            }
          }
        }
      },
      "DtoLanguageModel": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "name of language model"
          }
        }
      },
      "DtoIndexMethodList": {
        "type": "object",
        "properties": {
          "results": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/DtoIndexMethod"
            }
          }
        }
      },
      "DtoIndexMethod": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "name of index method"
          }
        }
      },
      "DtoMlsDataset": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "description: dataset name, e.g., tasks, documents"
          },
          "parameters": {
            "type": "array",
            "description": "List of mls-api parameters",
            "items": {
              "$ref": "#/components/schemas/DtoParameter"
            }
          }
        }
      },
      "DtoParameter": {
        "type": "object",
        "properties": {
          "parameter_name": {
            "type": "string",
            "description": "mls-api parameter"
          }
        }
      },
      "DtoMlsObject": {
        "type": "object",
        "properties": {
          "object_id": {
            "type": "string"
          }
        }
      },
      "DtoDatasetRequest": {
        "required": [
          "dataset_name"
        ],
        "type": "object",
        "properties": {
          "dataset_name": {
            "type": "string"
          }
        }
      },
      "DtoDenseEncodeQueryInput": {
        "required": [
          "model_name",
          "text"
        ],
        "type": "object",
        "properties": {
          "model_name": {
            "type": "string",
            "default": "bert-base-german-cased"
          },
          "text": {
            "type": "string",
            "default": "Elektrotechnik 1 Versuch 8: Wirkleistung von Wechselspannungen; Wirkleistung der Sinuswechselspannung in der praktischen Übung"
          }
        }
      },
      "DtoDenseEncodeQueryOutput": {
        "type": "object",
        "properties": {
          "dense_encode": {
            "type": "array",
            "items": {
              "type": "number"
            }
          }
        }
      },
      "DtoDenseEncodeDatasetInput": {
        "required": [
          "dataset_name",
          "model_name"
        ],
        "type": "object",
        "properties": {
          "model_name": {
            "type": "string",
            "default": "bert-base-german-cased"
          },
          "dataset_name": {
            "type": "string",
            "default": "mls-tasks"
          }
        }
      },
      "DtoDenseEncodeDatasetOutput": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        }
      },
      "DtoMLSIndexUpdateRequest": {
        "type": "object",
        "properties": {
          "datasets": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "DtoBM25IndexerRequest": {
        "type": "object",
        "properties": {
          "dataset_name": {
            "type": "string"
          },
          "json_collection": {
            "type": "string",
            "default": "JsonCollection"
          },
          "generator": {
            "type": "string",
            "default": "DefaultLuceneDocumentGenerator"
          },
          "threads": {
            "minimum": 1,
            "type": "integer"
          }
        }
      },
      "DtoIndexerRequest": {
        "type": "object",
        "properties": {
          "encode_type": {
            "type": "string",
            "default": "e.g., dense, traditional"
          },
          "model_name": {
            "type": "string",
            "default": "bert-base-german-cased"
          },
          "index_method": {
            "type": "string",
            "default": "e.g. flat-l2, flat-ip"
          },
          "dataset_name": {
            "type": "string",
            "default": "e.g. mls-tasks"
          }
        }
      },
      "DtoIndexerResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        }
      },
      "DtoDenseSearchRequest": {
        "required": [
          "query"
        ],
        "type": "object",
        "properties": {
          "user_id": {
            "type": "string",
            "description": "user ID"
          },
          "owner": {
            "type": "string",
            "description": "company ID"
          },
          "query": {
            "type": "string",
            "default": "Elektrotechnik 1 Versuch 8: Wirkleistung von Wechselspannungen; Wirkleistung der Sinuswechselspannung in der praktischen Übung"
          },
          "language_model": {
            "type": "string",
            "default": "bert-base-german-cased"
          },
          "index_method": {
            "type": "string",
            "default": "flat-l2"
          },
          "dataset_name": {
            "type": "string",
            "default": "mls-tasks"
          },
          "nr_result": {
            "minimum": 1,
            "type": "integer"
          }
        }
      },
      "DtoDenseSearchResponseList": {
        "type": "object",
        "properties": {
          "results": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/DtoDenseSearchResponse"
            }
          }
        }
      },
      "DtoDenseSearchResponse": {
        "type": "object",
        "properties": {
          "user_id": {
            "type": "string",
            "description": "user ID"
          },
          "owner": {
            "type": "string",
            "description": "company ID"
          },
          "id": {
            "type": "string"
          },
          "similarity": {
            "type": "number"
          }
        }
      }
    },
    "responses": {
      "MaskError": {
        "description": "When any error occurs on mask",
        "content": {}
      },
      "ParseError": {
        "description": "When a mask can't be parsed",
        "content": {}
      }
    }
  }
}